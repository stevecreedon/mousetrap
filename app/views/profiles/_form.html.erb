<%= form_for @profile, :url => user_profile_path(@profile.user), :multipart => true do |form| %>
	<div>
		<%= form.label :name %><br/>
		<%= form.text_field :name %>
	</div>
	<div>
		<%= form.label :description %><br/>
		<%= form.text_area :description %>
	</div>
	<div>
		<%= form.label :photo %><br/>
		<%= form.file_field :photo %>
	</div>
	<div>
		<%= form.submit %>
	</div>
<% end %>
